//Method to reverse a Linked List (unidirectional)

Node rverseList(Node head)
{
  	Node current=head;
  	Node previous=null;
  	Node next=null;
	
	while(current!=null)
	{
		next=current.next;
		current.next=previous;
		previous=current;
		current=next;
	}
	head=previous;
	return head;
}

//Method to reverse a Linked List (Bidirectional)

