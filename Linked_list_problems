//Method to reverse a Linked List (unidirectional)
class Node
{
	private int data;
	public Node next=null;
	
	public void setData(int data)
	{
		this.data=data;
	}
	public int getData()
	{
		return data;
	}
}

Node rverseList(Node head)
{
  	Node current=head;
  	Node previous=null;
  	Node next=null;
	
	while(current!=null)
	{
		next=current.next;
		current.next=previous;
		previous=current;
		current=next;
	}
	head=previous;
	return head;
}

//Method to reverse a Linked List (Bidirectional)
class Node
{
	
}
